function isProvisionedByUnitedInternet() {
var provcode = "";
 
provcode = "004.000.000.000";
 
if (provcode == "" || provcode == "000.000.000.000") {
return false;
}
var provider = GetFullProviderStringUsernameOnly(g_boxInfo.username);
return IsUnitedInternet(provider);
}
function ProviderDisplay (registrar) {
switch (registrar) {
case "freenet.de": return "freenet";
case "arcor.de": return "Arcor";
case "sipgate.de": return "sipgate";
case "sipkom.com": return "sipkom";
case "tel.congstar.de": return "congstar";
case "tel2.congstar.de": return "congstar";
case "1und1.de": return "1&1 Internet";
case "sip.1und1.de": return "1&1 Internet";
case "gmx.de": return "GMX";
case "reg.aolphone.aol.de": return "AOL";
case "a1.net": return "A1 over IP";
case "aolphone.aol.de": return "AOL";
case "strato-iphone.de": return "Strato";
case "proxy.dus.net": return "DUS.net";
case "telefon.unitymedia.de": return "Unitymedia";
case "kabelphone.de": return "Kabel Deutschland";
case "voip.inode.at": //return "Inode";
case "osvoip.upc.at": return "Inode";
case "sip.xs4all.nl": return "XS4ALL";
 
}
if (registrar.indexOf("dbd-") >= 0) {
return registrar.indexOf("dsl") >= 0 ? "DBD (DSL)" : "DBD (WiMAX)";
} else if (registrar.search("kabelphone.de") != "-1") {
return "Kabel Deutschland";
}
return registrar;
}
 
var g_Variante = "1und1";
var g_expertMode = "1";
var g_mldDelete = "Die Internetrufnummer wird jetzt gelöscht.";
var g_mldDeleteRUL = "Für die Internetrufnummer sind noch Rufumleitungen eingerichtet. Bitte löschen Sie diese bevor Sie die Internetrufnummer entfernen.";
var g_txtKDG = "Diese Internetrufnummer wurde von Ihrem Anbieter automatisch eingerichtet. Wenden Sie sich bitte an Kabel Deutschland, um die gewünschte Änderung vornehmen zu lassen.";
var g_MaxVoIpAccounts=20;
function uiDoOnLoad() {
document.title = "FRITZ!Box Internettelefonie";
var some_found = false;
var empty_found = false;
var bKDGProv = uiCheckKDGVoipConfig();
var curProvider = "";
 
if ("111" != "") {
jslDisplay("uiViewRow0", true);
uiSetViewActivated("uiViewActivsip0", "uiPostActivsip0");
curProvider="sip.1und1.de";
if (bKDGProv && curProvider.search("kabelphone.de")!=-1)
{
jslDisable("uiViewActivsip0");
}
if ("0"=="1")
{
jslDisable("uiViewActivsip0");
}
some_found = true;
} else {
empty_found = true;
}
 
if ("222" != "") {
jslDisplay("uiViewRow1", true);
uiSetViewActivated("uiViewActivsip1", "uiPostActivsip1");
curProvider="sip.1und1.de";
if (bKDGProv && curProvider.search("kabelphone.de")!=-1)
{
jslDisable("uiViewActivsip1");
}
if ("0"=="1")
{
jslDisable("uiViewActivsip1");
}
some_found = true;
} else {
empty_found = true;
}
 
if ("333" != "") {
jslDisplay("uiViewRow2", true);
uiSetViewActivated("uiViewActivsip2", "uiPostActivsip2");
curProvider="personal-voip.de";
if (bKDGProv && curProvider.search("kabelphone.de")!=-1)
{
jslDisable("uiViewActivsip2");
}
if ("0"=="1")
{
jslDisable("uiViewActivsip2");
}
some_found = true;
} else {
empty_found = true;
}
 
var n = Number("3");
if ((n >= g_MaxVoIpAccounts) && (!empty_found)) jslDisable("uiViewNewButton");
if (!some_found) jslDisplay("uiViewNoRow", true);
jslZebrastreifen("tSips");
}
function uiSetViewActivated(idui, idpost) {
jslSetChecked(idui, jslGetValue(idpost) == "1");
}
function ProviderDisplay (registrar, proxy) {
var providerId = getSipProviderByRegistrar(registrar, proxy);
if (providerId == "other") {
return registrar;
}
return g_sip.getDisplayName(providerId);
}
function AuswahlDisplay (nr) {
if (nr == "9") return "*120#";
return "*12"+(Number(nr)+1)+"#";
}
function trClass (n, id) {
var str = "<tr class=\"";
str += (Number(n)%2 == 0) ? "bcWeiss":"bcGrau";
str += "\" id=\"" + id + "\" style=\"display:none\">";
return str;
}
function uiOnChangeActivated(idui, idpost) {
var rule = idpost.substring(11);
if (uiFindKDG(rule)) return;
jslSetCheckValue(idpost, idui);
}
function uiDoSave() {
 
jslSetValue("uiPostActivsip0", jslGetChecked("uiViewActivsip0") ? "1":"0");
 
jslSetValue("uiPostActivsip1", jslGetChecked("uiViewActivsip1") ? "1":"0");
 
jslSetValue("uiPostActivsip2", jslGetChecked("uiViewActivsip2") ? "1":"0");
 
jslFormSubmitEx("fon", "siplist");
}
function uiDoCancel() {
jslGoTo("fon", "siplist");
}
function uiCheckKDGVoipConfig () {
var tr069ConfUrl = "https://acs2.online.de";
if (tr069ConfUrl.search("kabel-deutschland") != -1) {
jslSetValue("uiPostKDGCfg", "1");
jslEnable("uiPostKDGCfg");
return true;
}
return false;
}
function uiDoHelp() {
jslPopHelp("hilfe_fon_internetliste");
}
function uiFindKDG(rule) {
var tr069ConfUrl = "https://acs2.online.de";
if (tr069ConfUrl.search("kabel-deutschland") != -1) {
var reg="";
 
if (rule == "sip0") {
reg = "sip.1und1.de";
if (reg.search("kabelphone.de") != -1) {
alert(g_txtKDG);
return true;
}
}
 
if (rule == "sip1") {
reg = "sip.1und1.de";
if (reg.search("kabelphone.de") != -1) {
alert(g_txtKDG);
return true;
}
}
 
if (rule == "sip2") {
reg = "personal-voip.de";
if (reg.search("kabelphone.de") != -1) {
alert(g_txtKDG);
return true;
}
}
 
}
return false;
}
function uiDoEdit(rule) {
if (uiFindKDG(rule)) return;
jslSetValue('uiPostSipnr', rule);
var telcfg = "SIP"+jslGetValue("uiPostId"+rule);
jslSetValue('uiPostTelnr', telcfg);
if (rule=="sip0" && jslGetValue("uiPostDisplaysip0") == "") {
jslSetValue("uiPostIsNew", "1");
} else {
jslSetValue("uiPostIsNew", "0");
}
jslFormSubmitEx("fon", "sip1", "siplist");
}
function uiDoNew(rule) {
var empty_found = false;
//var telcfg = "SIP"+"3";
var telcfg = rule.toUpperCase();
 
if ("111" == "") {
if (!empty_found) {
rule = "sip0";
//telcfg = "SIP"+"0";
telcfg = "SIP"+"0";
empty_found = true;
}
}
 
if ("222" == "") {
if (!empty_found) {
rule = "sip1";
//telcfg = "SIP"+"1";
telcfg = "SIP"+"1";
empty_found = true;
}
}
 
if ("333" == "") {
if (!empty_found) {
rule = "sip2";
//telcfg = "SIP"+"2";
telcfg = "SIP"+"2";
empty_found = true;
}
}
 
jslSetValue('uiPostSipnr', rule);
jslSetValue('uiPostTelnr', telcfg);
jslSetValue('uiPostIsNew', "1");
jslFormSubmitEx("fon", "sip1", "siplist");
}
function uiDoDelete(rule,sipnummer)
{
if (uiFindKDG(rule)) return;
if (!confirm(g_mldDelete)) return;
var sipNo="";
if ( rule.indexOf("sip")==0)
{
sipNo = parseInt(rule.substr(3));
if (!uiCheckRUL(sipNo, sipnummer))
{
jslFormSubmitEx("fon", "rulall");
return;
}
uiDelete(sipNo, sipnummer);
}
switch (rule)
{
case "sip0":
jslSetValue("uiPostActivsip0", "0");
jslSetEnabled("uiPostDisplaysip0", true);
jslSetValue("uiPostDisplaysip0", "");
jslSetEnabled("uiPostRegistrarsip0", true);
jslSetValue("uiPostRegistrarsip0", "");
jslSetEnabled("uiPostUsernamesip0", true);
jslSetValue("uiPostUsernamesip0", "");
jslSetEnabled("uiPostPasswordsip0", true);
jslSetValue("uiPostPasswordsip0", "");
jslSetEnabled("uiPostStunserversip0", true);
jslSetValue("uiPostStunserversip0", "");
break;
case "sip1":
jslSetValue("uiPostActivsip1", "0");
jslSetEnabled("uiPostDisplaysip1", true);
jslSetValue("uiPostDisplaysip1", "");
jslSetEnabled("uiPostRegistrarsip1", true);
jslSetValue("uiPostRegistrarsip1", "");
jslSetEnabled("uiPostUsernamesip1", true);
jslSetValue("uiPostUsernamesip1", "");
jslSetEnabled("uiPostPasswordsip1", true);
jslSetValue("uiPostPasswordsip1", "");
jslSetEnabled("uiPostStunserversip1", true);
jslSetValue("uiPostStunserversip1", "");
break;
default:
jslSetEnabled('uiPostDelete'+rule, true);
jslSetEnabled('uiPostActiv'+rule, false);
break;
}
jslSetName("uiPostDeleteMSN", "telcfg:settings/SIP"+jslGetValue("uiPostId"+rule)+"/MSN");
jslEnable("uiPostDeleteMSN");
jslFormSubmitEx("fon", "siplist");
}
