eval('document.uiPostForm.'+id_to+'.value = document.uiViewForm.'+id_from+'.value');
} else {
var h1 = document.getElementById(id_to);
var h2 = document.getElementById(id_from);
if (h1 != null && h2 != null) h1.value=h2.value;
}
}
