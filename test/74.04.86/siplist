<script type="text/javascript"> 
function jslFormSetPost(name) {
var frm = document.forms[name];
if (frm) {
frm.method = "POST";
}
}
function jslFormSetGet(name) {
var frm = document.forms[name];
if (frm) {
frm.method = "GET";
}
}
function jslFormSubmit(name) {
UpdateDateTime();
var frm = document.forms[name];
if (frm) {
frm.submit();
}
}
function goToLuaPage(page) {
page += "?sid=3a8ef4a34cfcf8a7";
location.href = page;
}
