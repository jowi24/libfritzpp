function uiDelete(sipNo, sipnummer) {
// Internet Wahlregeln entfernen
var i=0;
var j=0;
 
//Callthrough deaktivieren
if (jslGetValue("uiPostCtMsn") == "SIP"+sipNo || jslGetValue("uiPostWeiterleiten") == "SIP"+sipNo) {
jslSetValue("uiPostCtAktiv","0");
jslEnable("uiPostCtAktiv");
}
//Analoge Nebenstellen (outgoing number entfernen)
 
if (jslGetValue("uiPostNr00") == "SIP"+sipNo) {jslSetValue("uiPostNr00", ""); jslEnable("uiPostNr00");}
 
if (jslGetValue("uiPostNr01") == "SIP"+sipNo) {jslSetValue("uiPostNr01", ""); jslEnable("uiPostNr01");}
 
if (jslGetValue("uiPostNr02") == "SIP"+sipNo) {jslSetValue("uiPostNr02", ""); jslEnable("uiPostNr02");}
 
if (jslGetValue("uiPostNr03") == "SIP"+sipNo) {jslSetValue("uiPostNr03", ""); jslEnable("uiPostNr03");}
 
if (jslGetValue("uiPostNr04") == "SIP"+sipNo) {jslSetValue("uiPostNr04", ""); jslEnable("uiPostNr04");}
 
if (jslGetValue("uiPostNr05") == "SIP"+sipNo) {jslSetValue("uiPostNr05", ""); jslEnable("uiPostNr05");}
 
if (jslGetValue("uiPostNr06") == "SIP"+sipNo) {jslSetValue("uiPostNr06", ""); jslEnable("uiPostNr06");}
 
if (jslGetValue("uiPostNr07") == "SIP"+sipNo) {jslSetValue("uiPostNr07", ""); jslEnable("uiPostNr07");}
 
if (jslGetValue("uiPostNr08") == "SIP"+sipNo) {jslSetValue("uiPostNr08", ""); jslEnable("uiPostNr08");}
 
