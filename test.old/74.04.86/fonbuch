jslGoTo("", "fonbuch2", "fonbuch");
}
else {
jslGoTo("", "fonbuch2_new", "fonbuch");
}
}
function GetFreeCode () {
var n = 0;
var i;
var c = 0;
while (c<99) {
c = c+1;
var code_besetzt = false;
for (i=0; i<n; i++) {
if (jslGetValue("uiPostCode"+i) == c) code_besetzt = true;
}
if (!code_besetzt) {
if (c < 10) return "0"+c;
return c;
}
}
return "01";
}
function GetFreeCode1() {
var returnvalue="23";
if(returnvalue=="" || returnvalue=="er"){
return "";
}
return returnvalue;
}
